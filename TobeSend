Get-VBRJob | Select-Object Name,
    @{N='HasSchedule';E={
        if ($_.ScheduleOptions.OptionsDaily.Enabled -or
            $_.ScheduleOptions.OptionsMonthly.Enabled -or
            $_.ScheduleOptions.OptionsPeriodically.Enabled -or
            $_.ScheduleOptions.OptionsContinuous.Enabled) {
            "Yes"
        } else {
            "No"
        }
    }}
